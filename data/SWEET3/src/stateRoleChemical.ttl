<http://sweetontology.net/stateRoleChemical> a owl:Ontology ;
	owl:versionInfo "3.0.0" ;
	rdfs:label "SWEET Ontology" ;
	owl:imports <http://sweetontology.net/prop> , <http://sweetontology.net/stateRole> .

crole:ChemicalRole a owl:Class ;
