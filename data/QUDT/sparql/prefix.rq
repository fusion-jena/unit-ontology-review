PREFIX qudt: <http://qudt.org/schema/qudt#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?prefix
                ?factor
                ?label
                ?labelLang
WHERE {
  ?prefix a                         qudt:PrefixUnit ;
          qudt:conversionMultiplier ?factor ;
          rdfs:label                ?label .
  
  BIND( LANG(?label) AS ?labelLang )
}